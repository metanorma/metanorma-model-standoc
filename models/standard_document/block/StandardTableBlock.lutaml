class StandardTableBlock {
  +width: String[0..1] {
    definition {
      Width of the table block in rendering.
    }
  }
  +number: String[0..1] {
    definition {
      Override the numbering of this block in numbering.
    }
  }
  +colgroup: TableCol[0..*] {
    definition {
      The widths of the columns in the table.
    }
  }
  +source: BlockSource[0..*] {
    definition {
      A URI or other reference intended to link to an externally hosted data source (or equivalent).
    }
  }
}
